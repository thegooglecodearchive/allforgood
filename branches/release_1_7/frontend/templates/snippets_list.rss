{% for result in result_set.clipped_results %}<item>
<title>{{ result.title|escape }}</title>
<link>{{ result.xml_url|escape }}</link>
<description>{{ result.snippet|escape }}</description>
<pubDate>{{ result.pubDate|escape  }}</pubDate>
<guid>{{ result.xml_url|escape }}</guid>
<fp:id>{{ result.id|escape  }}</fp:id>
{# groupid is a stable ID for the dedup'd set of results, #}
{#   including same listing but different time/location #}
<fp:groupid>{{ result.merge_key|escape  }}</fp:groupid>
<fp:provider>{{ result.provider|escape  }}</fp:provider>
<fp:startDate>{{ result.startdate|escape  }}</fp:startDate>
<fp:endDate>{{ result.enddate|escape  }}</fp:endDate>
<fp:base_url>{{ result.base_url|escape  }}</fp:base_url>
<fp:xml_url>{{ result.xml_url|escape  }}</fp:xml_url>
<fp:url_short>{{ result.url_short|escape  }}</fp:url_short>
<fp:latlong>{{ result.latlong|escape  }}</fp:latlong>
<fp:location_name>{{ result.location_string|escape  }}</fp:location_name>
{# fp:interest didn't make sense-- API is anonymous so there can't per per-user interest #}
<fp:interest_count>{{ result.interest_count|escape  }}</fp:interest_count>
<fp:impressions>{{ result.impressions|escape  }}</fp:impressions>
<fp:quality_score>{{ result.quality_score|escape  }}</fp:quality_score>
<fp:categories>{{ result.categories_api_str|escape  }}</fp:categories>
<fp:skills>{{ result.skills|escape  }}</fp:skills>
<fp:virtual>{{ result.virtual|escape  }}</fp:virtual>
<!-- addr1 may change; contact core eng team before using -->
<fp:addr1>{{ result.addr1|escape  }}</fp:addr1>
<!-- addrname1 may change; contact core eng team before using -->
<fp:addrname1>{{ result.addrname1|escape  }}</fp:addrname1>
<fp:sponsoringOrganizationName>{{ result.orgName|escape  }}</fp:sponsoringOrganizationName>
<fp:openEnded>{{ result.openEnded|escape  }}</fp:openEnded>
<fp:startTime>{{ result.startTime|escape  }}</fp:startTime>
<fp:endTime>{{ result.endTime|escape  }}</fp:endTime>
<fp:contactNoneNeeded>{{ result.contactNoneNeeded|escape  }}</fp:contactNoneNeeded>
<fp:contactEmail>{{ result.contactEmail|escape  }}</fp:contactEmail>
<fp:contactPhone>{{ result.contactPhone|escape  }}</fp:contactPhone>
<fp:contactName>{{ result.contactName|escape  }}</fp:contactName>
<fp:detailUrl>{{ result.detailUrl|escape  }}</fp:detailUrl>
{# TODO: make something better than weekly #}
{# TODO: make something better than biweekly #}
{# TODO: make something better than recurrence #}
<fp:audienceAll>{{ result.audienceAll|escape  }}</fp:audienceAll>
<fp:audienceAge>{{ result.audienceAge|escape  }}</fp:audienceAge>
<fp:minAge>{{ result.minAge|escape  }}</fp:minAge>
<fp:audienceSexRestricted>{{ result.audienceSexRestricted|escape  }}</fp:audienceSexRestricted>
{# TODO: sexRestrictedTo - ref policy team #}
{# TODO: make something better than commitmentHoursPerWeek #}
<fp:street1>{{ result.street1|escape  }}</fp:street1>
<fp:street2>{{ result.street2|escape  }}</fp:street2>
<fp:city>{{ result.city|escape  }}</fp:city>
<fp:region>{{ result.region|escape  }}</fp:region>
<fp:postalCode>{{ result.postalCode|escape  }}</fp:postalCode>
<fp:country>{{ result.country|escape  }}</fp:country>
</item>{% endfor %}
